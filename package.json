  "repository": {
    "type": "git",
    "url": "git@github.com:ppsirius/prox-agency.git"
  },
  "scripts": {
    "clear": "rimraf dist/",
    "build": "npm run clear && webpack",
    "build-prod": "npm run clear && cross-env NODE_ENV=production webpack -p",
    "watch": "npm run build -- --watch",
    "analyze-build-debug": "cross-env ANALYZE_BUNDLE=1 npm run build-debug",
    "analyze-build-prod": "cross-env ANALYZE_BUNDLE=1 npm run build-prod",
    "server-dev": "webpack-dev-server --open",
    "server-prod": "cross-env NODE_ENV=production webpack-dev-server -p --open"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-env": "^1.6.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "fullpage.js": "^2.9.5",
    "gsap": "^1.20.3",
    "jquery": "^3.2.1",
    "normalize.css": "^7.0.0",
    "slick-carousel": "^1.8.1"
  },
  "browserslist": [
    "last 8 versions"
  ]
}
